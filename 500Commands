call g:CommanderText("Key Sequence","Description")
call g:CommanderText("--- Basic Motion ---","---")
call g:CommanderText("h","Move cursor left one character")
call g:CommanderText("l","Move cursor right one character")
call g:CommanderText("j","Move cursor down one line")
call g:CommanderText("k","Move cursor up one line")
call g:CommanderText("0","Move cursor to the beginning of the line (column 0)")
call g:CommanderText("^","Move cursor to the first non-blank character of the line")
call g:CommanderText("$","Move cursor to the end of the line")
call g:CommanderText("g_","Move cursor to the last non-blank character of the line")
call g:CommanderText("w","Move cursor forward to the start of the next word")
call g:CommanderText("W","Move cursor forward to the start of the next WORD (space-separated)")
call g:CommanderText("b","Move cursor backward to the start of the previous word")
call g:CommanderText("B","Move cursor backward to the start of the previous WORD (space-separated)")
call g:CommanderText("e","Move cursor forward to the end of the current word")
call g:CommanderText("E","Move cursor forward to the end of the current WORD (space-separated)")
call g:CommanderText("ge","Move cursor backward to the end of the previous word")
call g:CommanderText("gE","Move cursor backward to the end of the previous WORD (space-separated)")
call g:CommanderText("gg","Go to the first line of the file")
call g:CommanderText("G","Go to the last line of the file")
call g:CommanderText("5G","Go to line 5 (replace 5 with any number)")
call g:CommanderText(":5","Go to line 5 (Ex command)")
call g:CommanderText("H","Move cursor to the top of the screen (High)")
call g:CommanderText("M","Move cursor to the middle of the screen (Middle)")
call g:CommanderText("L","Move cursor to the bottom of the screen (Low)")
call g:CommanderText("f{char}","Find next occurrence of {char} on the current line")
call g:CommanderText("F{char}","Find previous occurrence of {char} on the current line")
call g:CommanderText("t{char}","Move cursor *to* just before the next occurrence of {char}")
call g:CommanderText("T{char}","Move cursor *to* just after the previous occurrence of {char}")
call g:CommanderText(";","Repeat the last f, F, t, or T command")
call g:CommanderText(",","Repeat the last f, F, t, or T command in the reverse direction")
call g:CommanderText("%","Jump to matching parenthesis, bracket, or brace")
call g:CommanderText("(","Jump to previous sentence")
call g:CommanderText(")","Jump to next sentence")
call g:CommanderText("{","Jump to previous paragraph")
call g:CommanderText("}","Jump to next paragraph")
call g:CommanderText("[[","Jump to previous function or block")
call g:CommanderText("]]","Jump to next function or block")
call g:CommanderText("gj","Move down one *display* line (for wrapped text)")
call g:CommanderText("gk","Move up one *display* line (for wrapped text)")
call g:CommanderText("g0","Move to the beginning of the *display* line")
call g:CommanderText("g^","Move to the first non-blank char of the *display* line")
call g:CommanderText("g$","Move to the end of the *display* line")
call g:CommanderText("--- Scrolling ---","---")
call g:CommanderText("CTRL-F","Scroll forward one full screen")
call g:CommanderText("CTRL-B","Scroll backward one full screen")
call g:CommanderText("CTRL-D","Scroll down (forward) half a screen")
call g:CommanderText("CTRL-U","Scroll up (backward) half a screen")
call g:CommanderText("CTRL-E","Scroll down one line")
call g:CommanderText("CTRL-Y","Scroll up one line")
call g:CommanderText("zz","Center the current line on the screen")
call g:CommanderText("zt","Move current line to the top of the screen")
call g:CommanderText("zb","Move current line to the bottom of the screen")
call g:CommanderText("--- Entering Insert Mode ---","---")
call g:CommanderText("i","Enter Insert mode before the cursor")
call g:CommanderText("I","Enter Insert mode at the beginning of the line")
call g:CommanderText("a","Enter Insert mode after the cursor")
call g:CommanderText("A","Enter Insert mode at the end of the line")
call g:CommanderText("o","Open a new line below the current one and enter Insert mode")
call g:CommanderText("O","Open a new line above the current one and enter Insert mode")
call g:CommanderText("s","Substitute (delete) the character under the cursor and enter Insert mode")
call g:CommanderText("S","Substitute (delete) the entire current line and enter Insert mode (same as cc)")
call g:CommanderText("--- Insert Mode Commands ---","---")
call g:CommanderText("CTRL-H","Delete the previous character (like backspace)")
call g:CommanderText("CTRL-W","Delete the previous word")
call g:CommanderText("CTRL-U","Delete from the cursor to the start of the line")
call g:CommanderText("CTRL-T","Indent the current line (add shiftwidth)")
call g:CommanderText("CTRL-D","Un-indent the current line (remove shiftwidth)")
call g:CommanderText("CTRL-O","Execute a single Normal mode command, then return to Insert mode")
call g:CommanderText("CTRL-P","Autocomplete word (previous match)")
call g:CommanderText("CTRL-N","Autocomplete word (next match)")
call g:CommanderText("CTRL-X CTRL-F","Autocomplete file path")
call g:CommanderText("CTRL-X CTRL-L","Autocomplete whole line")
call g:CommanderText("CTRL-V {char}","Insert a special character literally (e.g., CTRL-V <Tab>)")
call g:CommanderText("<Esc>","Exit Insert mode and return to Normal mode")
call g:CommanderText("CTRL-[","Exit Insert mode (canonical alternative to <Esc>)")
call g:CommanderText("--- Editing in Normal Mode (Operators) ---","---")
call g:CommanderText("d","Delete operator")
call g:CommanderText("c","Change operator (delete, then enter Insert mode)")
call g:CommanderText("y","Yank operator (copy)")
call g:CommanderText(">","Indent operator")
call g:CommanderText("<","Un-indent operator")
call g:CommanderText("gU","Operator to make text uppercase")
call g:CommanderText("gu","Operator to make text lowercase")
call g:CommanderText("g~","Operator to swap case")
call g:CommanderText("=","Auto-indent operator")
call g:CommanderText("!","Filter operator (send text to external command)")
call g:CommanderText("r{char}","Replace the character under the cursor with {char}")
call g:CommanderText("R","Enter Replace mode (overwrite characters)")
call g:CommanderText("x","Delete the character under the cursor (shorthand for dl)")
call g:CommanderText("X","Delete the character before the cursor (shorthand for dh)")
call g:CommanderText("p","Paste *after* the cursor (from default register)")
call g:CommanderText("P","Paste *before* the cursor (from default register)")
call g:CommanderText("gp","Paste *after* the cursor and move cursor to end of pasted text")
call g:CommanderText("gP","Paste *before* the cursor and move cursor to end of pasted text")
call g:CommanderText("J","Join the current line with the next line (adds a space)")
call g:CommanderText("gJ","Join the current line with the next line (no space)")
call g:CommanderText("u","Undo the last change")
call g:CommanderText("U","Undo all changes on the current line")
call g:CommanderText("CTRL-R","Redo the last undone change")
call g:CommanderText(".","Repeat the last change")
call g:CommanderText("~","Swap the case of the character under the cursor and move right")
call g:CommanderText("--- Operator + Motion Combinations ---","---")
call g:CommanderText("dw","Delete from cursor to the start of the next word")
call g:CommanderText("db","Delete from cursor to the start of the previous word")
call g:CommanderText("de","Delete from cursor to the end of the current word")
call g:CommanderText("d$","Delete from cursor to the end of the line")
call g:CommanderText("d0","Delete from cursor to the beginning of the line")
call g:CommanderText("d^","Delete from cursor to the first non-blank char of the line")
call g:CommanderText("dd","Delete the current line")
call g:CommanderText("D","Delete from cursor to the end of the line (shorthand for d$)")
call g:CommanderText("cw","Change from cursor to the start of the next word")
call g:CommanderText("cb","Change from cursor to the start of the previous word")
call g:CommanderText("ce","Change from cursor to the end of the current word")
call g:CommanderText("c$","Change from cursor to the end of the line")
call g:CommanderText("c0","Change from cursor to the beginning of the line")
call g:CommanderText("c^","Change from cursor to the first non-blank char of the line")
call g:CommanderText("cc","Change the current line")
call g:CommanderText("C","Change from cursor to the end of the line (shorthand for c$)")
call g:CommanderText("yw","Yank (copy) from cursor to the start of the next word")
call g:CommanderText("yb","Yank from cursor to the start of the previous word")
call g:CommanderText("ye","Yank from cursor to the end of the current word")
call g:CommanderText("y$","Yank from cursor to the end of the line")
call g:CommanderText("y0","Yank from cursor to the beginning of the line")
call g:CommanderText("y^","Yank from cursor to the first non-blank char of the line")
call g:CommanderText("yy","Yank (copy) the current line")
call g:CommanderText("Y","Yank (copy) the current line (shorthand for yy)")
call g:CommanderText("dgg","Delete from current line to the top of the file")
call g:CommanderText("dG","Delete from current line to the end of the file")
call g:CommanderText("cgg","Change from current line to the top of the file")
call g:CommanderText("cG","Change from current line to the end of the file")
call g:CommanderText("ygg","Yank from current line to the top of the file")
call g:CommanderText("yG","Yank from current line to the end of the file")
call g:CommanderText("d/foo","Delete from cursor to next occurrence of 'foo'")
call g:CommanderText("c/foo","Change from cursor to next occurrence of 'foo'")
call g:CommanderText("y/foo","Yank from cursor to next occurrence of 'foo'")
call g:CommanderText("dfx","Delete from cursor up to and including 'x' on the line")
call g:CommanderText("dtx","Delete from cursor up to 'x' on the line")
call g:CommanderText("cfx","Change from cursor up to and including 'x' on the line")
call g:CommanderText("ctx","Change from cursor up to 'x' on the line")
call g:CommanderText("yfx","Yank from cursor up to and including 'x' on the line")
call g:CommanderText("ytx","Yank from cursor up to 'x' on the line")
call g:CommanderText(">>","Indent the current line")
call g:CommanderText("<<","Un-indent the current line")
call g:CommanderText(">G","Indent from current line to the end of the file")
call g:CommanderText("<G","Un-indent from current line to the end of the file")
call g:CommanderText("=G","Auto-indent from current line to the end of the file")
call g:CommanderText("gUe","Make text uppercase to the end of the word")
call g:CommanderText("gU$","Make text uppercase to the end of the line")
call g:CommanderText("gUw","Make text uppercase for one word")
call g:CommanderText("gUU","Make the entire line uppercase")
call g:CommanderText("gue","Make text lowercase to the end of the word")
call g:CommanderText("gu$","Make text lowercase to the end of the line")
call g:CommanderText("guw","Make text lowercase for one word")
call g:CommanderText("guu","Make the entire line lowercase")
call g:CommanderText("g~e","Swap case to the end of the word")
call g:CommanderText("g~$","Swap case to the end of the line")
call g:CommanderText("g~w","Swap case for one word")
call g:CommanderText("g~~","Swap case for the entire line")
call g:CommanderText("!G","Filter all lines from cursor to end of file through an external command")
call g:CommanderText("!!{cmd}","Filter the current line through external command {cmd}")
call g:CommanderText("--- Text Objects (used with d, c, y, v) ---","---")
call g:CommanderText("iw","Inner Word (text object for word, excluding whitespace)")
call g:CommanderText("aw","A Word (text object for word, including whitespace)")
call g:CommanderText("iW","Inner WORD (text object for WORD, excluding whitespace)")
call g:CommanderText("aW","A WORD (text object for WORD, including whitespace)")
call g:CommanderText("is","Inner Sentence")
call g:CommanderText("as","A Sentence")
call g:CommanderText("ip","Inner Paragraph")
call g:CommanderText("ap","A Paragraph")
call g:CommanderText("i\"","Inner Double Quotes (content inside )")
call g:CommanderText("a\"","A Double Quotes (content + the )")
call g:CommanderText("i'","Inner Single Quotes (content inside )")
call g:CommanderText("a'","A Single Quotes (content + the )")
call g:CommanderText("i`","Inner Backticks (content inside )")
call g:CommanderText("a`","A Backticks (content + the ``)")
call g:CommanderText("i(","Inner Parentheses (content inside ())")
call g:CommanderText("a(","A Parentheses (content + the ())")
call g:CommanderText("ib","Inner Block (same as i(), short for 'inner block')")
call g:CommanderText("ab","A Block (same as a(), short for 'a block')")
call g:CommanderText("i[","Inner Brackets (content inside [])")
call g:CommanderText("a[","A Brackets (content + the [])")
call g:CommanderText("i{","Inner Braces (content inside {})")
call g:CommanderText("a{","A Braces (content + the {})")
call g:CommanderText("iB","Inner Block (same as i{, short for 'inner Braces')")
call g:CommanderText("aB","A Block (same as a{, short for 'a Braces')")
call g:CommanderText("i<","Inner Angle Brackets (content inside <>)")
call g:CommanderText("a<","A Angle Brackets (content + the <>)")
call g:CommanderText("it","Inner Tag (content inside an HTML/XML tag)")
call g:CommanderText("at","A Tag (content + the tag)")
call g:CommanderText("--- Text Object Combinations (Examples) ---","---")
call g:CommanderText("diw","Delete inner word (cursor can be anywhere in word)")
call g:CommanderText("daw","Delete a word (deletes word and one space)")
call g:CommanderText("ciw","Change inner word")
call g:CommanderText("caw","Change a word")
call g:CommanderText("yiw","Yank inner word")
call g:CommanderText("yaw","Yank a word")
call g:CommanderText("viw","Visually select inner word")
call g:CommanderText("vaw","Visually select a word")
call g:CommanderText("di\"","Delete inside double quotes")
call g:CommanderText("da\"","Delete a double-quoted string (including quotes)")
call g:CommanderText("ci\"","Change inside double quotes")
call g:CommanderText("ca\"","Change a double-quoted string")
call g:CommanderText("yi\"","Yank inside double quotes")
call g:CommanderText("ya\"","Yank a double-quoted string")
call g:CommanderText("di(","Delete inside parentheses")
call g:CommanderText("da(","Delete a parenthesized block (including parentheses)")
call g:CommanderText("ci(","Change inside parentheses")
call g:CommanderText("ca(","Change a parenthesized block")
call g:CommanderText("yi(","Yank inside parentheses")
call g:CommanderText("ya(","Yank a parenthesized block")
call g:CommanderText("dib","Delete inside block ()")
call g:CommanderText("dab","Delete a block ()")
call g:CommanderText("di[","Delete inside brackets")
call g:CommanderText("da[","Delete a bracketed block")
call g:CommanderText("ci[","Change inside brackets")
call g:CommanderText("di{","Delete inside braces")
call g:CommanderText("da{","Delete a brace block")
call g:CommanderText("ci{","Change inside braces")
call g:CommanderText("ca{","Change a brace block")
call g:CommanderText("diB","Delete inside Brace block {}")
call g:CommanderText("daB","Delete a Brace block {}")
call g:CommanderText("dit","Delete inner tag (HTML/XML)")
call g:CommanderText("dat","Delete a tag (HTML/XML)")
call g:CommanderText("cit","Change inner tag")
call g:CommanderText("cat","Change a tag")
call g:CommanderText("dip","Delete inner paragraph")
call g:CommanderText("dap","Delete a paragraph")
call g:CommanderText("cip","Change inner paragraph")
call g:CommanderText("cap","Change a paragraph")
call g:CommanderText("dis","Delete inner sentence")
call g:CommanderText("das","Delete a sentence")
call g:CommanderText("cis","Change inner sentence")
call g:CommanderText("cas","Change a sentence")
call g:CommanderText("--- Visual Mode (Entering) ---","---")
call g:CommanderText("v","Enter Visual mode (character-wise)")
call g:CommanderText("V","Enter Visual mode (line-wise)")
call g:CommanderText("CTRL-V","Enter Visual Block mode (column-wise)")
call g:CommanderText("gv","Reselect the last visual selection")
call g:CommanderText("viw","Visually select inner word")
call g:CommanderText("vas","Visually select a sentence")
call g:CommanderText("vap","Visually select a paragraph")
