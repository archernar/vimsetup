call g:CommanderText("vit","Visually select inner tag block")
call g:CommanderText("--- Visual Mode (Commands *in* Visual Mode) ---","---")
call g:CommanderText("o","Go to Other end of the visual selection")
call g:CommanderText("O","Go to other end of visual selection (block mode)")
call g:CommanderText("d","Delete the selected text")
call g:CommanderText("c","Change the selected text")
call g:CommanderText("y","Yank (copy) the selected text")
call g:CommanderText("J","Join all selected lines (adds spaces)")
call g:CommanderText("gJ","Join all selected lines (no spaces)")
call g:CommanderText(">","Indent the selected lines")
call g:CommanderText("<","Un-indent the selected lines")
call g:CommanderText("u","Make selected text lowercase")
call g:CommanderText("U","Make selected text uppercase")
call g:CommanderText("~","Swap case of selected text")
call g:CommanderText("=","Auto-indent the selected lines")
call g:CommanderText("r{char}","Replace every selected character with {char}")
call g:CommanderText(":","Enter Ex command mode for the selected range (shows :'<,'>)")
call g:CommanderText("!{cmd}","Filter selected lines through external command {cmd}")
call g:CommanderText("I","(In Block Mode) Enter Insert mode at the beginning of *each* selected line")
call g:CommanderText("A","(In Block Mode) Enter Insert mode at the end of *each* selected line")
call g:CommanderText("s","(In Block Mode) Delete the block and enter Insert mode (same as c)")
call g:CommanderText("C","(In Block Mode) Delete the block from cursor to end of lines and enter Insert mode")
call g:CommanderText("--- Search & Replace ---","---")
call g:CommanderText("/pattern","Search forward for 'pattern'")
call g:CommanderText("?pattern","Search backward for 'pattern'")
call g:CommanderText("n","Repeat last search in the same direction")
call g:CommanderText("N","Repeat last search in the opposite direction")
call g:CommanderText("*","Search forward for the whole word under the cursor")
call g:CommanderText("#","Search backward for the whole word under the cursor")
call g:CommanderText("g*","Search forward for the partial word under the cursor")
call g:CommanderText("g#","Search backward for the partial word under the cursor")
call g:CommanderText(":s/old/new/","Replace first 'old' with 'new' on the current line")
call g:CommanderText(":s/old/new/g","Replace all 'old' with 'new' on the current line")
call g:CommanderText(":s/old/new/i","Replace 'old' with 'new' on current line (case-insensitive)")
call g:CommanderText(":s/old/new/c","Replace 'old' with 'new' on current line (with confirmation)")
call g:CommanderText(":%s/old/new/g","Replace all 'old' with 'new' in the entire file")
call g:CommanderText(":%s/old/new/gc","Replace all 'old' with 'new' in the file, with confirmation")
call g:CommanderText(":'a,'bs/old/new/g","Replace all 'old' with 'new' between marks 'a' and 'b'")
call g:CommanderText(":.,+5s/old/new/g","Replace all 'old' with 'new' from current line to 5 lines down")
call g:CommanderText(":g/pattern/s/old/new/g","Run a substitution on all lines that match 'pattern'")
call g:CommanderText(":v/pattern/s/old/new/g","Run a substitution on all lines that *do not* match 'pattern'")
call g:CommanderText(":noh","Clear search highlighting (no highlight)")
call g:CommanderText("--- Marks and Jumps ---","---")
call g:CommanderText("m{a-z}","Set mark {a-z} at cursor position (local to file)")
call g:CommanderText("m{A-Z}","Set mark {A-Z} at cursor position (global, across files)")
call g:CommanderText("'{a-z}","Jump to the *line* of mark {a-z} (first non-blank char)")
call g:CommanderText("`{a-z}`","Jump to the *exact position* (line and column) of mark {a-z}")
call g:CommanderText("'{A-Z}","Jump to the *line* of global mark {A-Z}")
call g:CommanderText("`{A-Z}`","Jump to the *exact position* of global mark {A-Z}")
call g:CommanderText("''","Jump to the *line* of the last jump")
call g:CommanderText("``","Jump to the *exact position* of the last jump")
call g:CommanderText("'.","Jump to the *line* of the last edit")
call g:CommanderText("`.","Jump to the *exact position* of the last edit")
call g:CommanderText("gi","Jump to the *exact position* of the last insert and enter insert mode")
call g:CommanderText(":marks","List all current marks")
call g:CommanderText("CTRL-O","Go to older position in the jump list (go 'back')")
call g:CommanderText("CTRL-I","Go to newer position in the jump list (go 'forward')")
call g:CommanderText(":jumps","List all positions in the jump list")
call g:CommanderText("CTRL-]","Jump to the definition of the tag under the cursor (uses tags file)")
call g:CommanderText("CTRL-T","Jump back from a tag definition")
call g:CommanderText("--- Registers ---","---")
call g:CommanderText("\"{register}y","Yank into a specific {register} (e.g., \"ayy)")
call g:CommanderText("\"{register}d","Delete into a specific {register} (e.g., \"add)")
call g:CommanderText("\"{register}p","Paste from a specific {register} (e.g., \"ap)")
call g:CommanderText("\"{register}P","Paste before from a specific {register} (e.g., \"aP)")
call g:CommanderText("\"Ayy","Append the current line to register 'a' (use uppercase letter)")
call g:CommanderText("\"Add","Append-delete the current line to register 'a'")
call g:CommanderText(":reg","Display the contents of all registers")
call g:CommanderText(":reg {register}","Display the contents of a specific {register}")
call g:CommanderText("\"\"","The unnamed register (default for y, d, p)")
call g:CommanderText("\"0","The yank register (holds the last yank, not overwritten by delete)")
call g:CommanderText("\"1-9","The numbered registers (hold last 9 deletes, 1 is newest)")
call g:CommanderText("\"a-z","The named registers (user-controlled)")
call g:CommanderText("\"A-Z","The named registers (appends to the lowercase version)")
call g:CommanderText("\"*","The system clipboard register (selection clipboard, X11)")
call g:CommanderText("\"+","The system clipboard register (primary clipboard)")
call g:CommanderText("\".","The last inserted text register")
call g:CommanderText("\"%","The current file name register")
call g:CommanderText("\":","The last Ex command register")
call g:CommanderText("\"/","The last search pattern register")
call g:CommanderText("\"_","The black hole register (deletes to here are gone forever, e.g. \"_dd)")
call g:CommanderText("--- Macros ---","---")
call g:CommanderText("q{a-z}","Start recording a macro into register {a-z}")
call g:CommanderText("q","Stop recording a macro (while recording)")
call g:CommanderText("@{a-z}","Execute the macro from register {a-z}")
call g:CommanderText("@@","Repeat the last executed macro")
call g:CommanderText("10@{a-z}","Execute macro 'a' 10 times")
call g:CommanderText("10@@","Execute the last macro 10 times")
call g:CommanderText("--- Ex Commands (File) ---","---")
call g:CommanderText(":w","Write (save) the current file")
call g:CommanderText(":w!","Write (save) the current file, force overwrite")
call g:CommanderText(":w {filename}","Write to {filename} (Save As)")
call g:CommanderText(":.w {filename}","Write current line to {filename}")
call g:CommanderText(":'a,'bw {filename}","Write lines from mark 'a' to 'b' to {filename}")
call g:CommanderText(":q","Quit the current buffer (fails if unsaved)")
call g:CommanderText(":q!","Quit the current buffer (discard changes)")
call g:CommanderText(":wq","Write (save) and quit")
call g:CommanderText(":x","Write (if changed) and quit (same as :wq)")
call g:CommanderText("ZZ","(Normal Mode) Write (if changed) and quit")
call g:CommanderText("ZQ","(Normal Mode) Quit and discard changes")
call g:CommanderText(":qa","Quit all buffers (fails if any are unsaved)")
call g:CommanderText(":qa!","Quit all buffers (discard all changes)")
call g:CommanderText(":wqa","Write (save) and quit all buffers")
call g:CommanderText(":xa","Write (if changed) and quit all buffers")
call g:CommanderText(":e {filename}","Edit {filename} in the current buffer")
call g:CommanderText(":e!","Reload the current file from disk (discard changes)")
call g:CommanderText(":enew","Edit a new, unnamed buffer")
call g:CommanderText(":f","Show current file name and status")
call g:CommanderText(":f {filename}","Rename current file to {filename}")
call g:CommanderText("--- Ex Commands (Buffers) ---","---")
call g:CommanderText(":ls","List all open buffers")
call g:CommanderText(":b {N}","Go to buffer {N} (from :ls)")
call g:CommanderText(":b {name}","Go to buffer {name} (can use partial name)")
call g:CommanderText(":bnext","Go to the next buffer in the list")
call g:CommanderText(":bprev","Go to the previous buffer in the list")
call g:CommanderText(":bfirst","Go to the first buffer in the list")
call g:CommanderText(":blast","Go to the last buffer in the list")
call g:CommanderText(":bd","Delete (close) the current buffer")
call g:CommanderText(":%bd","Delete (close) all buffers")
call g:CommanderText(":sp","Split the window horizontally (new window shows current buffer)")
call g:CommanderText(":sp {filename}","Split the window horizontally and open {filename}")
call g:CommanderText(":vsp","Split the window vertically")
