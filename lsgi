cat /dev/stdin | while read -r file; do
#   if [[ "$file" == "." || "$file" == ".." ]]; then
#       continue
#   fi
    if [[ "$file" == "." ]]; then
        continue
    fi
    if [[ "$file" == ".." ]]; then
        continue
    fi
    if [[ "$file" == ".git" ]]; then
        continue
    fi
    # -q silences output; '!' inverts the check
    if ! git check-ignore -q "$file"; then
        echo "$file"
    fi
done
